# Fireflies-Supabase RAG Worker Environment Variables Template
# Copy this file to .env.local and fill in your values

# Supabase Configuration
SUPABASE_URL=https://[your-project-ref].supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Hyperdrive Configuration
HYPERDRIVE_ID=your-hyperdrive-id-here

# Fireflies Configuration
FIREFLIES_API_KEY=your-fireflies-api-key-here
FIREFLIES_WEBHOOK_SECRET=optional-webhook-secret

# KV Namespace IDs
KV_NAMESPACE_ID=your-kv-namespace-id-here
KV_PREVIEW_ID=your-kv-preview-id-here

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Sync Configuration
SYNC_BATCH_SIZE=25
VECTOR_CACHE_TTL=3600

# Features
ENABLE_REALTIME=false

# Database Connection (for Hyperdrive setup)
DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:5432/postgres

# Development
NODE_ENV=development
LOG_LEVEL=info